{
  "entryPoints": [
    "src"
  ],
  "entryPointStrategy": "expand",
  "out": "docs",
  "exclude": [
    "**/node_modules/**",
    "**/out/**",
    "**/test/**",
    "**/media/**"
  ],
  "excludeExternals": true,
  "externalPattern": [
    "**/node_modules/**"
  ],
  "excludePrivate": true,
  "excludeProtected": false,
  "includeVersion": true,
  "skipErrorChecking": true
}
